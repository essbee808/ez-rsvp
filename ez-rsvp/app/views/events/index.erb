<head>
  <link rel="stylesheet" type="text/css" href="../stylesheets/main.css" />
</head>
<div class="content">
	<h3 class="heading">List of Events</h3>
	<br>
		<% @events.each do |el| %>
			<% @el = el %>
			<a href="/events/<%= @el.id %>"><li><%= el.title %></li></a>
		<% end %>
	<br>
	<br>
	<a href="/events/new" class="submit-button">Create New Event</a>
	<br>
	<br>
</div>

<a href="/logout">Logout</a>