<div class="my-events">
	<h2 class="title">RSVP List</h2>
	<div class="list">
		<% @user.rsvps.each do |el| %>
				<ul>
					<li class="item"><a href="/events/<%= el.id %>"><%= el.event.title %></a></li>
				</ul>
				<form action="/rsvp/<%= el.event.id %>" method="POST">
			        <input id="hidden" type="hidden" name="_method" value="DELETE">
			        <input type="submit" value="Remove" class="remove">
		    	</form>
		    	<br>
		<% end %>
	</div>
</div>

<div class="col-3">
      <a href="/events">More Events</a>
</div>